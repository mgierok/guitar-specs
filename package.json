{
  "name": "guitar-specs-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "esbuild web/static/src/main.js --bundle --watch --outdir=web/static/dist/js --format=esm",
    "build": "esbuild web/static/src/main.js --bundle --minify --outdir=web/static/dist/js --format=esm && npm run build:css",
    "build:css": "tailwindcss -i web/static/src/style.css -o web/static/dist/css/style.css --minify",
    "watch:css": "tailwindcss -i web/static/src/style.css -o web/static/dist/css/style.css --watch"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  },
  "dependencies": {
    "htmx.org": "^2.0.6",
    "alpinejs": "^3.13.3"
  }
}
